- content_for :title do
  = t 'devise.passwords.link'
  
- semantic_form_for resource_name, resource, :url => password_path(resource_name) do |f|
  - f.error_messages
  - f.inputs do
    = f.input :email      
  - f.buttons do
    = f.commit_button :password
  
.links
  = render :partial => "shared/devise_links"
